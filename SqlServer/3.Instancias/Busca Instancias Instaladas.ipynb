            "source": [
                "#<b>Script para recuperar as instancias instaladas em um ambiente on-primesse</b>"
            ],
            "metadata": {
                "azdata_cell_guid": "dd877f91-141a-42e4-97aa-409f3002adfe"
            }
        },
        {
            "cell_type": "code",
            "source": [
                "\r\n",
                "\r\n",
                "EXECUTE sys.xp_regread @rootkey = 'HKEY_LOCAL_MACHINE',\r\n",
                "                       @key = 'SOFTWARE\\Microsoft\\Microsoft SQL Server',\r\n",
                "                       @value_name = 'InstalledInstances';"
            ],
            "metadata": {
